{
  "name": "Exercise Reminder",
  "version": "1.0",
  "description": "Set a timer to take a movement break.",
  "permissions": ["storage", "alarms", "notifications"],
  "web_accessible_resources": ["images/otter2.png"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/otter2.png"
  },

  "icons": {
    "16": "images/otter2.png",
    "32": "images/otter2.png",
    "48": "images/otter2.png",
    "128": "images/otter2.png"
  },
  "manifest_version": 2
}
